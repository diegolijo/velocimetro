    "@ionic/angular": "^6.1.9",
    "@ionic/cordova-builders": "^7.0.0",
    "@ionic/storage": "^3.0.6",
    "angular-speech-to-text": "^1.0.20",
    "date-fns": "^2.29.3",
    "import": "^0.0.6",
    "ionicons": "^6.0.3",
    "rxjs": "~7.5.0",
    "tesseract.js": "^4.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
