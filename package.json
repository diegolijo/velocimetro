{
  "name": "velocimetro",
  "version": "1.0.0",
  "author": "Parrino",
  "homepage": "https://www.vayapedal.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "update-vosk": "ionic cordova plugins rm vosk-speech-to-text && ionic cordova plugins add vosk-speech-to-text",
    "update-notifications": "ionic cordova plugins rm cordova-notification-listener && ionic cordova plugins add cordova-notification-listener",
    "create_cordova_folder": "node -e \"require('fs').mkdirSync( './www', { recursive: true });\"",
    "build:platform": "npm run create_cordova_folder && cordova platform rm android && cordova platform add android@10.1.1 && ionic cordova run android"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@awesome-cordova-plugins/android-full-screen": "^6.3.0",
    "@awesome-cordova-plugins/android-permissions": "^6.3.0",
    "@awesome-cordova-plugins/camera": "^6.3.0",
    "@awesome-cordova-plugins/core": "^6.3.0",
    "@awesome-cordova-plugins/geolocation": "^6.3.0",
    "@awesome-cordova-plugins/http": "^6.3.0",
    "@awesome-cordova-plugins/location-accuracy": "^6.3.0",
    "@awesome-cordova-plugins/native-audio": "^6.3.0",
    "@awesome-cordova-plugins/native-geocoder": "^6.3.0",
